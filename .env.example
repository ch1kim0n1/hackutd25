# APEX Environment Configuration
# Copy this file to .env and fill in your actual API keys

# OpenRouter API (for AI agents using NVIDIA models)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# Alpaca Trading API (for live/paper trading)
# Get your keys at: https://app.alpaca.markets/paper/dashboard/overview
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here

# Redis Connection (for orchestrator state)
REDIS_URL=redis://localhost:6379

# Optional: Plaid API (for personal finance features)
# PLAID_CLIENT_ID=your_plaid_client_id
# PLAID_SECRET=your_plaid_secret
# PLAID_ENV=sandbox

# Development Settings
ELECTRON_START_URL=http://localhost:3000
BACKEND_URL=http://localhost:8000
